    {
      "role": "client",
      "time": "2021-02-12T12:50:25",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-02-12T12:50:28",
      "utterance": "睡眠はとれていますか?"
    },
    {
      "role": "client",
      "time": "2021-02-12T12:50:45",
      "utterance": "一応、5~6時間は眠れています"

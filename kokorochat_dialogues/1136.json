    {
      "role": "client",
      "time": "2021-01-08T14:19:28",
      "utterance": "はい、5人家族です"
    },
    {
      "role": "counselor",
      "time": "2021-01-08T14:19:44",
      "utterance": "そうなんですね"
    },
    {
      "role": "client",
      "time": "2021-01-08T14:20:18",
      "utterance": "声も聴きたくないし、一緒の空気も吸いたくないです"

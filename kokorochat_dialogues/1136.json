    {
      "role": "counselor",
      "time": "2021-01-08T14:08:20",
      "utterance": "そうなんですね"
    },
    {
      "role": "counselor",
      "time": "2021-01-08T14:09:32",
      "utterance": "それが原因だと、思われていますか"
    },
    {
      "role": "client",
      "time": "2021-01-08T14:09:57",
      "utterance": "はい、そこの女性となので"

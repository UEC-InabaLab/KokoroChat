    {
      "role": "counselor",
      "time": "2021-01-08T14:04:49",
      "utterance": "それはいつ頃ですか"
    },
    {
      "role": "client",
      "time": "2021-01-08T14:05:06",
      "utterance": "たぶんずっと"
    },
    {
      "role": "client",
      "time": "2021-01-08T14:05:27",
      "utterance": "10年くらいだと思います"

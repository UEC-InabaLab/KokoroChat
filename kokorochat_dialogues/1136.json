    {
      "role": "client",
      "time": "2021-01-08T14:26:55",
      "utterance": "まだしていません"
    },
    {
      "role": "client",
      "time": "2021-01-08T14:27:02",
      "utterance": "口もきいていないので"
    },
    {
      "role": "counselor",
      "time": "2021-01-08T14:27:06",

    {
      "role": "client",
      "time": "2021-10-19T19:32:26",
      "utterance": "はい。"
    },
    {
      "role": "counselor",
      "time": "2021-10-19T19:32:37",
      "utterance": "誰にも会いたくない"
    },
    {
      "role": "client",
      "time": "2021-10-19T19:32:56",
      "utterance": "はい。"
    },
    {
      "role": "counselor",
      "time": "2021-10-19T19:33:06",
      "utterance": "人が信じられない"

{
  "dialogue": [
    {
      "role": "counselor",
      "time": "2021-01-27T18:54:11",
      "utterance": "こんばんは、本日はよろしくお願いいたします"
    },
    {
      "role": "client",
      "time": "2021-01-27T18:57:05",
      "utterance": "よろしくお願いします。"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T18:57:44",

    {
      "role": "counselor",
      "time": "2021-01-27T19:30:50",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:30:55",
      "utterance": "タイプは違うので"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:31:28",
      "utterance": "心配な面も自分自身のこととは違うのですが、"

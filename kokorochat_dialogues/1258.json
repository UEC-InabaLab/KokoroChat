    {
      "role": "client",
      "time": "2021-01-27T19:02:59",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:03:41",
      "utterance": "よろしければ、どんなことが上手くいかないのか、もう少し具体的に教えていただけますか"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:04:56",
      "utterance": "考え方や性格が違いすぎて、お互いストレスがたまってしまいます。"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:05:03",
      "utterance": "もうひとつは、"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:05:12",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:06:35",
      "utterance": "娘が最低限の責任を持つことも出来てないので困っています。"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:07:16",
      "utterance": "そうなんですね、娘さんのことをそんなふうに感じておられるのですね、、"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:08:30",
      "utterance": "ちなみに娘さんはおいくつでしょうか"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:08:44",
      "utterance": "19歳です"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:09:37",
      "utterance": "そうなんですね最低限の責任が持ていないということですが、どんなところでそうお感じになるのでしょうか"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:11:52",
      "utterance": "自分がだらしないことを自覚出来ていないみたいなのですが、"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:12:36",
      "utterance": "日々の生活の中でだらしなくしていることが影響して"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:12:48",
      "utterance": "ええ"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:13:35",
      "utterance": "仕事のトラブルがあったり自分にとって師承をきたすと"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:13:54",
      "utterance": "うんうん"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:14:12",
      "utterance": "支障です"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:14:24",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:14:37",
      "utterance": "人のせいにしてしまうのです..."
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:15:44",
      "utterance": "そうなんですね、親からすると娘さんがそういった感じだと心配になりますよね、、"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:16:54",
      "utterance": "はい..."
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:17:24",
      "utterance": "ご自身はそんな姿を見た時は、どんな言葉をかけていますか"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:18:54",
      "utterance": "日々の生活習慣が大事なことで"
    },
    {
      "role": "counselor",
      "time": "2021-01-27T19:19:08",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:19:50",
      "utterance": "日々の過ごし方が仕事の面でも人と関わる時に自然に出ると思うから"
    },
    {
      "role": "client",
      "time": "2021-01-27T19:20:08",
      "utterance": "家でだらしなくしてて"

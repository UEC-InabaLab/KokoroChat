{
  "dialogue": [
    {
      "role": "counselor",
      "time": "2024-06-12T20:26:41",
      "utterance": "(本日は宜しくお願いします）"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:27:30",
      "utterance": "(こちらこそよろしくお願いします）"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:28:24",
      "utterance": "（扱いたいテーマがありましたら、遠慮なくおっしゃってください）"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:29:38",
      "utterance": "（ありがとうございます。お任せいたします）"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:30:10",
      "utterance": "（承知しました）"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:30:17",
      "utterance": "こんばんは初めに年齢、性別、今日お話ししたい事を教えて下さい"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:30:54",
      "utterance": "13才"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:30:57",
      "utterance": "男"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:31:14",
      "utterance": "死にたい"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:31:38",
      "utterance": "生きている意味がわからない"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:32:02",
      "utterance": "教えてくれてありがとうそんな風に思っているんですね辛いですね"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:32:20",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:32:55",
      "utterance": "中学生ですか？"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:33:00",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:33:35",
      "utterance": "どんな時そんな風に感じますか"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:34:05",
      "utterance": "ずっと"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:34:33",
      "utterance": "生きててもしかたない"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:35:16",
      "utterance": "ずっとなんですねうんうん、しかたないっておもうんですね"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:35:37",
      "utterance": "いつごろからそう思いようになりましたか"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:35:54",
      "utterance": "寝ててこのまま死ねたら楽慣れるのに"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:36:32",
      "utterance": "中学1年終わりぐらい"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:36:55",
      "utterance": "もういいよね"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:37:27",
      "utterance": "楽になりたいんですねそのくらいしんどい思いでいきているんですね"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:38:34",
      "utterance": "中一のとき、そんな気持ちになったのはきっかけはありましたか"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:38:35",
      "utterance": "しんどい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:38:59",
      "utterance": "うんうん、すこし一緒にどうしたらいいいか考えたいです"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:39:36",
      "utterance": "いろいろあってよくわからない"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:40:54",
      "utterance": "いろいろありすぎたのね、、、それは例えば学校のことですかおうちの事とかですか"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:41:05",
      "utterance": "両方かな"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:42:08",
      "utterance": "そうそれは考えるだけで疲れてしまうんだと思います今日まで悩んできたんですね"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:42:33",
      "utterance": "家も学校もどこにも居場所がない"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:43:11",
      "utterance": "誰もわかってくれない"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:43:27",
      "utterance": "詳しく話せる？話せるところだけでいいですよ"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:43:59",
      "utterance": "しんどいから無理"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:44:28",
      "utterance": "うんうん今はしんどいんですね大丈夫ですよ"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:45:08",
      "utterance": "生まれてこないほうが良かったんだ"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:45:39",
      "utterance": "僕が死んだ方がみんなに迷惑かけずに済む"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:45:54",
      "utterance": "これ以上生きてたらダメだと思う"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:46:10",
      "utterance": "そんな風に思うことがあったんですね迷惑をかけていると感じるの？"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:46:16",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:47:04",
      "utterance": "それがいやなんですね、、他にはなにか思っていることはありますか"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:47:21",
      "utterance": "僕が生きていたら周りを嫌な気持ちにさせてしまうから"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:47:40",
      "utterance": "だからいない方がいい"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:47:57",
      "utterance": "死んだ方がいい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:48:17",
      "utterance": "周りとは例えば誰の事をを考えていますか"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:48:29",
      "utterance": "親や友達"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:49:00",
      "utterance": "親はお父さん、お母さんどっちかな"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:49:17",
      "utterance": "両方"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:50:12",
      "utterance": "うんうんご両親、お友達に嫌な思いをさせたくないんですね"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:50:23",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:50:55",
      "utterance": "優しい気持ちをもっているんですね"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:51:02",
      "utterance": "僕が生きているだけで、嫌な思いをさせている"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:51:21",
      "utterance": "べつに優しくないよ"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:51:43",
      "utterance": "そんな風に思うのはどうしてですか"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:51:54",
      "utterance": "言われたから"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:52:18",
      "utterance": "言われたのね、、悲しかったですね、、"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:52:31",
      "utterance": "うん"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:53:56",
      "utterance": "そんなこと言われたら誰だって辛くて色々考えてしまいますよね今日まで我慢してきたんですね"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:54:30",
      "utterance": "我慢？分からない"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:55:01",
      "utterance": "もう生きてる意味がないから"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:55:12",
      "utterance": "早く死にたい"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:56:15",
      "utterance": "どうしたら死ねますか？"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:57:44",
      "utterance": "ネットで検索したけど、悩む"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:58:01",
      "utterance": "死なない方法もあると思いますよ"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:58:33",
      "utterance": "死にたいから"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T20:58:59",
      "utterance": "楽になりたいんですよね"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:59:21",
      "utterance": "楽？というより、死なないといけない"
    },
    {
      "role": "client",
      "time": "2024-06-12T20:59:51",
      "utterance": "だって、周りが嫌な思いをしてるから"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:00:43",
      "utterance": "生きていたらダメなんです"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:01:12",
      "utterance": "いけないの？死ぬしか嫌な思いしなくなる方法はないということはないですよ"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:01:13",
      "utterance": "存在自体がダメだから"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:01:49",
      "utterance": "いなくなればいいのにって"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:02:36",
      "utterance": "そういわれたのね誰に言われたかは聞いてもいいですか？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:02:45",
      "utterance": "友達"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:03:20",
      "utterance": "産むんじゃなかったって"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:03:26",
      "utterance": "お母さんから"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:03:44",
      "utterance": "どうして、、？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:04:21",
      "utterance": "分からない"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:04:44",
      "utterance": "教えてくれてありがとう"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:05:26",
      "utterance": "僕のせいでお母さんいなくなったんだって、頼むから消えてくれって"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:05:39",
      "utterance": "お父さんに言われた"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:07:30",
      "utterance": "どうしてあなたのせいなんでしょう、、お父さんは間違っているかもしれないですよ"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:08:04",
      "utterance": "お母さんがいなくなったんですね寂しかったでしょう"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:08:14",
      "utterance": "そうだと思う"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:09:22",
      "utterance": "お父さんはそうだね"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:09:44",
      "utterance": "そうとは、、、間違っているとおもう？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:10:46",
      "utterance": "お父さんはさみしかったと思うけど、ぼくはよくわからない"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:11:19",
      "utterance": "あなたの気持ちのことでしたね、、わからなかったのね"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:12:09",
      "utterance": "お母さんのイメージははどんな感じですか"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:12:50",
      "utterance": "かわいそうな人"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:13:17",
      "utterance": "どういうところでしょう"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:13:27",
      "utterance": "わからない"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:13:44",
      "utterance": "なんとなく"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:14:30",
      "utterance": "そうなのですね幸せそうではなかったということでしょうか"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:14:42",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:15:46",
      "utterance": "うんうん、おかあさんはどうなっていけばいいなと思っていますか"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:16:07",
      "utterance": "わからない"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:17:24",
      "utterance": "生きていたらいけない存在なんです"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:18:03",
      "utterance": "生まれたらいけない存在なんです"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:18:10",
      "utterance": "お父さんとお友達に言われたから？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:18:21",
      "utterance": "みんなに言われたから"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:18:44",
      "utterance": "他にもいるの？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:19:13",
      "utterance": "両親と友達から"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:19:30",
      "utterance": "お母さん？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:19:39",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:20:37",
      "utterance": "そうとても悲しかったと思います、、"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:20:53",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:21:26",
      "utterance": "どうしてそんなこと言ったのかはわからない？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:21:35",
      "utterance": "わからないです"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:22:22",
      "utterance": "きっと僕が何かしたんだと思います"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:23:15",
      "utterance": "死にたくなってしまう気持ちがわかりました心が苦しくて疲れてしまったんだと感じましたここまでお話を聞いて、、、"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:23:37",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:24:52",
      "utterance": "お母さんはとても何か苦しんでいたのかもしれないと思います。お父さんはそれをあなたのせいにしたいと思っているだけかもしれません"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:26:02",
      "utterance": "しんどいのはもうあんまりないよただ死にたいと思うだけ"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:26:54",
      "utterance": "そうかもしれないし、そうじゃないかもしれない考えるのもやめた"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:28:05",
      "utterance": "そっかそっか疲れたね"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:28:25",
      "utterance": "そうですね"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:28:42",
      "utterance": "でももう少し生きてみませんか何かが変ることもあるんですよ"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:28:58",
      "utterance": "？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:29:13",
      "utterance": "よくわからない"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:29:49",
      "utterance": "考え方がかわったり、生きてみたくなったりいま、とても疲れている中で決めてほしくないなと思います"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:30:42",
      "utterance": "生きているだけでダメな存在だから"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:31:33",
      "utterance": "嫌な思いをさせる"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:32:20",
      "utterance": "あなたにも迷惑をかけた"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:32:38",
      "utterance": "聴いてくれてありがとう"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:33:17",
      "utterance": "存在がだめなんて、そんなことないですよ私に？私は色々話してくれて、あなたに生きてほしいとおもいましたよ"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:33:56",
      "utterance": "優しくないと言っていたけど私はそう感じました"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:34:03",
      "utterance": "そうなんだ"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:34:38",
      "utterance": "嫌な事をいってきた人が正しいわけではないですよ"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:35:13",
      "utterance": "先生とか相談できるひとはいませんか？"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:35:36",
      "utterance": "今日は聴いてくれて"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:35:44",
      "utterance": "ありがとうございました。"
    },
    {
      "role": "counselor",
      "time": "2024-06-12T21:36:21",
      "utterance": "こちらこそ、、また話したいときはいつでも来てほしいです"
    },
    {
      "role": "client",
      "time": "2024-06-12T21:36:30",
      "utterance": "わかりました"
    },
    {

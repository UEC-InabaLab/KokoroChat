    {
      "role": "counselor",
      "time": "2020-10-16T14:39:47",
      "utterance": "でも、"
    },
    {
      "role": "client",
      "time": "2020-10-16T14:39:51",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2020-10-16T14:40:33",
      "utterance": "本当は、本当に平等に家事をやりたいと思っている"
    },
    {
      "role": "client",
      "time": "2020-10-16T14:40:48",
      "utterance": "そうですね"
    },
    {
      "role": "counselor",
      "time": "2020-10-16T14:41:24",
      "utterance": "なおみさんは、一生懸命で、真面目で、優しい方ですね"
    },
    {
      "role": "client",
      "time": "2020-10-16T14:41:49",
      "utterance": "そうですかね、、"

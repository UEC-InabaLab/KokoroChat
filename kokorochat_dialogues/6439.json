    {
      "role": "client",
      "time": "2024-02-14T10:20:40",
      "utterance": "え・・安心？"
    },
    {
      "role": "client",
      "time": "2024-02-14T10:21:10",
      "utterance": "仕事をしていないのは・・・いつからだろう"

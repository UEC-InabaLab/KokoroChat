    {
      "role": "client",
      "time": "2024-06-08T10:45:30",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-06-08T10:45:44",
      "utterance": "ひとりで耐えているとあなた自身もストレスが溜まってしまいますよね・・"

    {
      "role": "counselor",
      "time": "2022-01-30T10:56:32",
      "utterance": "ありがとうございます。よろしくお願いします。"
    },
    {
      "role": "client",
      "time": "2022-01-30T10:56:50",
      "utterance": "よろしくお願いします。"
    },
    {
      "role": "counselor",
      "time": "2022-01-30T10:57:16",

    {
      "role": "counselor",
      "time": "2021-01-12T21:45:33",
      "utterance": "何よりあなたのことが心配です。"
    },
    {
      "role": "client",
      "time": "2021-01-12T21:45:43",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-01-12T21:45:49",
      "utterance": "ありがとうございます"

    {
      "role": "counselor",
      "time": "2022-03-09T20:31:52",
      "utterance": "はい、そうなんですか？"
    },
    {
      "role": "client",
      "time": "2022-03-09T20:32:10",
      "utterance": "ええ"
    },
    {
      "role": "client",
      "time": "2022-03-09T20:32:39",

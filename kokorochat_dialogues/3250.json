    {
      "role": "client",
      "time": "2022-03-09T20:03:12",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2022-03-09T20:03:22",
      "utterance": "最近ではないですね。"
    },
    {
      "role": "client",
      "time": "2022-03-09T20:03:47",

    },
    {
      "role": "counselor",
      "time": "2023-04-10T13:34:10",
      "utterance": "ええ（聞いていますね）"
    },
    {
      "role": "client",
      "time": "2023-04-10T13:34:10",
      "utterance": "みんなみんな"
    },
    {
      "role": "client",
      "time": "2023-04-10T13:34:18",
      "utterance": "自分の思いをぶつけてくる"
    },
    {
      "role": "client",
      "time": "2023-04-10T13:34:27",
      "utterance": "重いことを話して"
    },
    {
      "role": "client",
      "time": "2023-04-10T13:34:32",
      "utterance": "スッキリしている"
    },
    {
      "role": "client",
      "time": "2023-04-10T13:34:37",

    {
      "role": "client",
      "time": "2021-03-01T16:46:27",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-03-01T16:46:52",
      "utterance": "これからのあなたの人生で、ずっとこの迷いを引きずってしまうことがとても心配です。"
    },
    {
      "role": "client",
      "time": "2021-03-01T16:47:20",
      "utterance": "そうですか"

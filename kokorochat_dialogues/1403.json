    {
      "role": "client",
      "time": "2021-03-01T16:17:50",
      "utterance": "そうです。"
    },
    {
      "role": "client",
      "time": "2021-03-01T16:18:01",
      "utterance": "あ、そこまで酷いわけではないんですが"

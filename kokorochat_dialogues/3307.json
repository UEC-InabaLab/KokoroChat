    {
      "role": "counselor",
      "time": "2022-03-21T20:30:44",
      "utterance": "孤独感なんですね。"
    },
    {
      "role": "client",
      "time": "2022-03-21T20:30:50",
      "utterance": "ええ"
    },
    {
      "role": "client",
      "time": "2022-03-21T20:30:59",

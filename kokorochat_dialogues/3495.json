    {
      "role": "counselor",
      "time": "2022-05-18T13:08:12",
      "utterance": "なるほどですね。"
    },
    {
      "role": "counselor",
      "time": "2022-05-18T13:08:53",
      "utterance": "成長するための大事なフィードバックですからね。"
    },
    {
      "role": "client",
      "time": "2022-05-18T13:09:16",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2022-05-18T13:10:03",

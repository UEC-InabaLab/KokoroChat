    {
      "role": "counselor",
      "time": "2022-05-18T13:15:20",
      "utterance": "とかでしょうか。"
    },
    {
      "role": "client",
      "time": "2022-05-18T13:15:31",
      "utterance": "そーですね。。"
    },
    {
      "role": "client",
      "time": "2022-05-18T13:15:38",
      "utterance": "自覚はめっちゃしてるので"

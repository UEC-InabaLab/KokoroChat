    {
      "role": "counselor",
      "time": "2021-04-28T15:30:18",
      "utterance": "そうなんですね。"
    },
    {
      "role": "client",
      "time": "2021-04-28T15:30:58",
      "utterance": "急にこんな事になって、なんでって。"
    },
    {
      "role": "counselor",
      "time": "2021-04-28T15:31:45",

    },
    {
      "role": "counselor",
      "time": "2021-04-28T15:42:40",
      "utterance": "どうしたら気持ちが楽になれるか一緒に考えていけたらと思います。"
    },
    {
      "role": "client",
      "time": "2021-04-28T15:42:48",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-04-28T15:43:04",
      "utterance": "楽になれますか?"
    },
    {
      "role": "counselor",
      "time": "2021-04-28T15:43:25",

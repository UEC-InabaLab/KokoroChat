    {
      "role": "client",
      "time": "2024-05-21T12:49:08",
      "utterance": "学校でもらったカードに書いてあったから"
    },
    {
      "role": "counselor",
      "time": "2024-05-21T12:49:33",
      "utterance": "そっか、そうなんだね。"

{
  "dialogue": [
    {
      "role": "counselor",
      "time": "2020-07-18T10:25:15",
      "utterance": "こんにちは。本日はよろしくお願いします。"
    },
    {
      "role": "client",
      "time": "2020-07-18T10:25:32",

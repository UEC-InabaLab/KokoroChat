    {
      "role": "client",
      "time": "2021-08-24T20:21:20",
      "utterance": "そうです"
    },
    {
      "role": "counselor",
      "time": "2021-08-24T20:21:39",
      "utterance": "何かありましたか？"
    },
    {
      "role": "client",
      "time": "2021-08-24T20:21:53",
      "utterance": "急だったのでわからないんです"
    },
    {
      "role": "counselor",
      "time": "2021-08-24T20:22:19",
      "utterance": "そうなんですね、理由がわからないって、つらいですよね・・・"
    },
    {
      "role": "client",
      "time": "2021-08-24T20:22:27",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-08-24T20:22:44",
      "utterance": "どうしたらいいでしょうか"
    },
    {
      "role": "counselor",
      "time": "2021-08-24T20:22:45",
      "utterance": "もう少し教えてください。"
    },
    {
      "role": "counselor",
      "time": "2021-08-24T20:23:24",
      "utterance": "どうしたらいいか、一緒に考えたいと思いますので、もう少し状況お聞きさせてください。"
    },
    {
      "role": "client",
      "time": "2021-08-24T20:24:10",
      "utterance": "今まで3人で話してたのに、急に2人で内緒話したり、手紙も2人で交換して、私にはまわしてくれません。"
    },
    {
      "role": "counselor",
      "time": "2021-08-24T20:24:56",
      "utterance": "そうなんですね、仲間から外されてる感じですか？"
    },
    {
      "role": "client",
      "time": "2021-08-24T20:25:04",
      "utterance": "そうです"
    },
    {
      "role": "client",
      "time": "2021-08-24T20:25:21",
      "utterance": "話しかけてもそっけないんです"

    },
    {
      "role": "client",
      "time": "2020-07-24T20:39:17",
      "utterance": "ええ"
    },
    {
      "role": "counselor",
      "time": "2020-07-24T20:40:06",
      "utterance": "いまは、まだスイッチ入ってない感じですよね"
    },
    {
      "role": "client",
      "time": "2020-07-24T20:40:16",
      "utterance": "そうですね"
    },
    {
      "role": "client",
      "time": "2020-07-24T20:40:30",

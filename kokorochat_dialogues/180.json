    {
      "role": "client",
      "time": "2020-07-15T12:21:16",
      "utterance": "はい。。。"
    },
    {
      "role": "counselor",
      "time": "2020-07-15T12:21:26",
      "utterance": "はい、お子さんを優先ですね。"

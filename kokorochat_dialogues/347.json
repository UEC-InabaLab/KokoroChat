    {
      "role": "client",
      "time": "2020-08-14T19:17:28",
      "utterance": "うんあってない"
    },
    {
      "role": "counselor",
      "time": "2020-08-14T19:18:04",
      "utterance": "それは辛いね・・・高校は嫌な事ばかりなのですね"
    },
    {
      "role": "client",
      "time": "2020-08-14T19:18:32",
      "utterance": "そうだね合わないって感じ"
    },
    {
      "role": "counselor",
      "time": "2020-08-14T19:18:58",
      "utterance": "合わないのに頑張っていってるんですね"
    },
    {
      "role": "client",
      "time": "2020-08-14T19:19:21",
      "utterance": "行ってない日が多いかな"
    },
    {
      "role": "counselor",
      "time": "2020-08-14T19:19:57",
      "utterance": "そうなのね休みたい時は休むのが一番ね"
    },
    {
      "role": "client",
      "time": "2020-08-14T19:19:57",
      "utterance": "公園とか街で時間潰してる"
    },
    {
      "role": "counselor",
      "time": "2020-08-14T19:20:23",
      "utterance": "時間つぶしも大変っていってましたね"
    },
    {
      "role": "client",
      "time": "2020-08-14T19:20:43",
      "utterance": "やることないから"
    },
    {
      "role": "counselor",
      "time": "2020-08-14T19:21:11",
      "utterance": "りんごさんは、一人行動が多いのかな？"
    },
    {
      "role": "client",
      "time": "2020-08-14T19:21:30",
      "utterance": "そうだね"
    },
    {
      "role": "counselor",
      "time": "2020-08-14T19:21:50",

    {
      "role": "counselor",
      "time": "2020-09-18T20:44:41",
      "utterance": "ええ"
    },
    {
      "role": "client",
      "time": "2020-09-18T20:44:51",
      "utterance": "いつもとは違う空気感でした"
    },
    {
      "role": "client",
      "time": "2020-09-18T20:45:11",
      "utterance": "いつもよりつかれましたね"
    },
    {
      "role": "counselor",
      "time": "2020-09-18T20:45:34",
      "utterance": "そうですよね。今まで経験したことがない、大変なことだったと思います"

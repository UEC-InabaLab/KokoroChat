{
  "dialogue": [
    {
      "role": "counselor",
      "time": "2024-06-10T20:50:33",
      "utterance": "(今晩は。宜しくお願い致します。)"
    },
    {
      "role": "client",
      "time": "2024-06-10T20:51:11",
      "utterance": "（こんばんは！こちらこそ宜しくお願い致します）"

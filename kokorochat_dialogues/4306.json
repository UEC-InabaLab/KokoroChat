    {
      "role": "counselor",
      "time": "2022-12-06T11:07:59",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2022-12-06T11:08:07",
      "utterance": "本当は会いたくなかった"
    },
    {
      "role": "client",
      "time": "2022-12-06T11:08:15",
      "utterance": "そんな風に言っていました。"

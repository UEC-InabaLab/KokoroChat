    {
      "role": "client",
      "time": "2022-01-10T15:39:39",
      "utterance": "ないと思います"
    },
    {
      "role": "client",
      "time": "2022-01-10T15:39:43",
      "utterance": "それと"
    },
    {
      "role": "counselor",

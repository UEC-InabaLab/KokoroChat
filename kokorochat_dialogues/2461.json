    {
      "role": "counselor",
      "time": "2021-09-16T10:51:04",
      "utterance": "そうなのですね。"
    },
    {
      "role": "counselor",
      "time": "2021-09-16T10:51:41",
      "utterance": "防止にたいして、積極的に行動されているのですね。"
    },
    {
      "role": "client",
      "time": "2021-09-16T10:51:58",
      "utterance": "はい子どももいますしね"
    },
    {
      "role": "counselor",
      "time": "2021-09-16T10:52:47",

    {
      "role": "counselor",
      "time": "2021-09-16T10:43:32",
      "utterance": "そうなのですね。"
    },
    {
      "role": "client",
      "time": "2021-09-16T10:43:58",
      "utterance": "こういう温度差は仕方ないんでしょうか"
    },
    {
      "role": "counselor",
      "time": "2021-09-16T10:44:31",
      "utterance": "悲しいですね。"
    },
    {
      "role": "counselor",
      "time": "2021-09-16T10:44:56",

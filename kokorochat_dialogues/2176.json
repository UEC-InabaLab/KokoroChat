    {
      "role": "client",
      "time": "2021-07-12T21:23:46",
      "utterance": "いえ私1人です"
    },
    {
      "role": "counselor",
      "time": "2021-07-12T21:24:29",
      "utterance": "そうだったんですねお一人暮らしをされておられる感じでしょうか"
    },
    {
      "role": "counselor",
      "time": "2021-07-12T21:24:45",
      "utterance": "それとも、ご同居者がおられる感じでしょうか"
    },
    {
      "role": "client",
      "time": "2021-07-12T21:24:49",
      "utterance": "今は1人です"
    },
    {
      "role": "counselor",
      "time": "2021-07-12T21:25:32",
      "utterance": "いまはお一人でいらっしゃるのですねご同居されている方はおられるのでしょうか"

{
  "dialogue": [
    {
      "role": "client",
      "time": "2020-07-23T18:00:22",
      "utterance": "よろしくお願いいたします。"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:04:49",
      "utterance": "こんばんは。"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:10:16",
      "utterance": "18時～19時ロールプレイ 只今待機中です。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:14:46",
      "utterance": "お願い致します。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:15:30",
      "utterance": "どの様なごそうだんですか"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:15:47",
      "utterance": "アイディアの事務局に問い合わせをしていたもので、少々お待ちください。"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:16:58",
      "utterance": "時間が過ぎていましたで｡もう少しお待ちいただけますか。すみません。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:17:30",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:18:28",
      "utterance": "アイディア事務局に相手の方から返答有と伝えました。"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:18:49",
      "utterance": "申し訳ございません。お時間いただきました。よろしくお願いいたします。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:19:27",
      "utterance": "よろしくお願いいたします。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:20:05",
      "utterance": "どのようなごそうだんですか"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:20:15",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:20:45",
      "utterance": "結婚を考えていたのですが、コロナの件で結婚式が延期になり"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:21:00",
      "utterance": "人生を考え直していたところで"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:21:11",
      "utterance": "そうでしたか"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:21:14",
      "utterance": "結婚するべきか"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:21:27",
      "utterance": "考え直すべきか"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:21:38",
      "utterance": "色々考えてしまいます"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:22:20",
      "utterance": "相手のかたのきもちは"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:22:40",
      "utterance": "相手の気持ちですか？"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:22:49",
      "utterance": "わかりません"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:23:04",
      "utterance": "自分のことで精一杯です"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:24:19",
      "utterance": "結婚まで考えたのですもの"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:24:27",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:25:45",
      "utterance": "状況を詳しくお聞きしたいのですが"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:25:54",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:27:24",
      "utterance": "何からお伝えしたら宜しいでしょうか。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:28:11",
      "utterance": "式場のよやくは"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:28:35",
      "utterance": "キャンセルしました"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:29:02",
      "utterance": "式の目処がつきませんので。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:29:29",
      "utterance": "そうですよね"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:29:36",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:31:01",
      "utterance": "何か彼に不満があるのですか"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:31:35",
      "utterance": "相手に不満はありません"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:32:22",
      "utterance": "結婚を迷っているおきもちは"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:32:39",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:34:47",
      "utterance": "何を一番解決したいのですか"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:35:35",
      "utterance": "そう言われますと"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:35:53",
      "utterance": "心を痛めているところは"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:36:09",
      "utterance": "自分の中で気持ちの整理がつかず考えてしまいます。"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:36:25",
      "utterance": "心は痛めていません"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:37:57",
      "utterance": "コロナのせいで迷っているのですね"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:38:36",
      "utterance": "コロナのせいではないと思っています。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:38:55",
      "utterance": "彼との話し合いは"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:39:04",
      "utterance": "実際にコロナで結婚式はキャンセルになりましたが"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:39:30",
      "utterance": "相手とは話していません"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:40:41",
      "utterance": "相手と話し合う前に解決できれば話しません"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:41:32",
      "utterance": "せっかく素晴らしい方とお知り合いになったのですもの"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:42:04",
      "utterance": "素晴らしい方ですか？"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:43:34",
      "utterance": "不満がない方ですもの"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:43:54",
      "utterance": "そう言われますと不満はありません。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:45:06",
      "utterance": "二人で今の気持ちをつたえてみては"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:45:34",
      "utterance": "そうですか"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:46:34",
      "utterance": "そうすることでお互いの気持ちがかくにんできます"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:47:18",
      "utterance": "気持ちを伝えられたら確認できますね。"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:47:30",
      "utterance": "素直に自分の気持ちを伝えられたら"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:48:29",
      "utterance": "相手と自分の温度差があるとき、急に話をしても大丈夫なのでしょうか？"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:49:09",
      "utterance": "自分の問題なのかなと思っていますので"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:49:40",
      "utterance": "これで関係が悪くなってもという思いもあります"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:49:49",
      "utterance": "今どき不満のない方などいませんし、一番は心をつたえてみるとよいでしょう"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:50:47",
      "utterance": "自分の気持ちを素直に伝えるにはどうしたら良いですか？"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:50:57",
      "utterance": "相手に不満はありません。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:53:05",
      "utterance": "そのような方ですものきっときもちをきてくださいますので勇気をだして"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:53:49",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:54:15",
      "utterance": "きっとわかってくださいます"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:54:40",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T18:56:38",
      "utterance": "お話ができたら結果をきかせてくださいね"
    },
    {
      "role": "client",
      "time": "2020-07-23T18:59:00",
      "utterance": "自分の気持ちに整理がつかずに考えている状況で何から話したら良いのでしょうか？"
    },
    {
      "role": "client",
      "time": "2020-07-23T19:04:43",
      "utterance": "勇気を出して話してみることですね。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T19:05:04",
      "utterance": "今の気持ちは相手には申し分ない方でしたが結婚式をとりやめた。今の気持ちにせいりがつかない"
    },
    {
      "role": "client",
      "time": "2020-07-23T19:05:19",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T19:06:00",
      "utterance": "そうです、ゆうきをだして"
    },
    {
      "role": "client",
      "time": "2020-07-23T19:06:18",
      "utterance": "わかりました。ありがとうございます。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T19:07:22",
      "utterance": "祈っていますね"
    },
    {
      "role": "client",
      "time": "2020-07-23T19:07:39",
      "utterance": "ありがとうございました。"
    },
    {
      "role": "counselor",
      "time": "2020-07-23T19:07:57",
      "utterance": "ありがとうございました。"
    }

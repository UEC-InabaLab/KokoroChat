    {
      "role": "client",
      "time": "2021-08-24T15:30:33",
      "utterance": "はい・・"
    },
    {
      "role": "client",
      "time": "2021-08-24T15:30:45",
      "utterance": "もう少し頑張ってみようかな"
    },
    {
      "role": "counselor",
      "time": "2021-08-24T15:31:02",
      "utterance": "チャッピーさんの気持ちが少しでも楽になれたらと思いますが、"
    },
    {
      "role": "counselor",
      "time": "2021-08-24T15:32:07",

    {
      "role": "counselor",
      "time": "2021-07-06T20:07:21",
      "utterance": "うんうん"
    },
    {
      "role": "client",
      "time": "2021-07-06T20:07:26",
      "utterance": "心が女か、と聞かれるとよくわからず、"

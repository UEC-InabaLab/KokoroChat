    {
      "role": "counselor",
      "time": "2021-07-06T20:06:06",
      "utterance": "はい"
    },
    {
      "role": "client",
      "time": "2021-07-06T20:06:19",
      "utterance": "でも、女装したいなっていう気持ちがあって、"
    },
    {
      "role": "client",
      "time": "2021-07-06T20:06:36",

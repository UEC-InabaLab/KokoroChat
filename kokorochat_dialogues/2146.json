    {
      "role": "client",
      "time": "2021-07-06T20:33:05",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-07-06T20:34:47",
      "utterance": "どう思われますか？"
    },
    {
      "role": "client",
      "time": "2021-07-06T20:35:52",

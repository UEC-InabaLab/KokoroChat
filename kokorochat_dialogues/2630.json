    {
      "role": "client",
      "time": "2021-10-29T10:50:59",
      "utterance": "はい、、"
    },
    {
      "role": "client",
      "time": "2021-10-29T10:51:07",
      "utterance": "まあ、、"
    },
    {
      "role": "counselor",
      "time": "2021-10-29T10:51:17",
      "utterance": "うんうん"
    },
    {
      "role": "counselor",
      "time": "2021-10-29T10:52:24",
      "utterance": "あなたは本当はどんなふうになったらいいなと思っているのかな"

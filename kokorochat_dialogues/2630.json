    {
      "role": "client",
      "time": "2021-10-29T10:54:46",
      "utterance": "よくわからない、、"
    },
    {
      "role": "counselor",
      "time": "2021-10-29T10:55:13",
      "utterance": "ごめんね、ちょっと難しかったよね"

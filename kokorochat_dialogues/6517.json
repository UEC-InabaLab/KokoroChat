    {
      "role": "client",
      "time": "2024-02-28T20:52:09",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2024-02-28T20:52:22",
      "utterance": "ご自分の心の中にあるお気持ちを、少しずつこうやって吐き出していくことで、気持ちが楽になることがあるのかもしれません。"
    },
    {
      "role": "client",
      "time": "2024-02-28T20:52:56",
      "utterance": "そういうこともあるんですね"
    },
    {
      "role": "counselor",
      "time": "2024-02-28T20:53:14",
      "utterance": "ええ、お話しすることが難しければ、紙に書き出してみたり"

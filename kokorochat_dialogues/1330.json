    {
      "role": "client",
      "time": "2021-02-06T21:15:39",
      "utterance": "はい。"
    },
    {
      "role": "counselor",
      "time": "2021-02-06T21:16:30",
      "utterance": "キャプテンに言うこともできないですか？"
    },

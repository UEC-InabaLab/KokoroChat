    {
      "role": "client",
      "time": "2022-02-09T16:05:26",
      "utterance": "と。"
    },
    {
      "role": "counselor",
      "time": "2022-02-09T16:05:48",
      "utterance": "ええ、ええ、"
    },
    {
      "role": "counselor",
      "time": "2022-02-09T16:06:03",

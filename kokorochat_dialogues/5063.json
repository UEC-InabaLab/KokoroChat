    {
      "role": "client",
      "time": "2023-05-16T10:57:44",
      "utterance": "1人の大人として気持ちを共有する"
    },
    {
      "role": "client",
      "time": "2023-05-16T10:58:02",
      "utterance": "今まで気づけませんでした"

    {
      "role": "counselor",
      "time": "2020-12-31T13:52:41",
      "utterance": "うん"
    },
    {
      "role": "client",
      "time": "2020-12-31T13:53:22",
      "utterance": "私は家族の中で一番成績よくないから"

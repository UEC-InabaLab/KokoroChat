    {
      "role": "client",
      "time": "2020-12-31T13:55:53",
      "utterance": "うん"
    },
    {
      "role": "counselor",
      "time": "2020-12-31T13:56:06",
      "utterance": "今日は、なんて呼べばいいですか？本名じゃなくても構いませんよ"
    },
    {
      "role": "client",
      "time": "2020-12-31T13:56:24",
      "utterance": "さよです"
    },
    {
      "role": "counselor",
      "time": "2020-12-31T13:56:46",

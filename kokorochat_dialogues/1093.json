    {
      "role": "client",
      "time": "2020-12-31T13:59:11",
      "utterance": "うん"
    },
    {
      "role": "counselor",
      "time": "2020-12-31T13:59:22",
      "utterance": "家だと、色々と、考えてしまいますものね。"

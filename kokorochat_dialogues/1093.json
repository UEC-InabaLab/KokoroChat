    {
      "role": "counselor",
      "time": "2020-12-31T13:53:30",
      "utterance": "うん、うん。"
    },
    {
      "role": "client",
      "time": "2020-12-31T13:54:03",
      "utterance": "お兄ちゃんもお姉ちゃんもいい大学行ってる"

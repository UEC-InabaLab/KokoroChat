    {
      "role": "counselor",
      "time": "2020-12-31T13:52:14",
      "utterance": "うん、うん"
    },
    {
      "role": "counselor",
      "time": "2020-12-31T13:52:37",
      "utterance": "続けてくださいね言える範囲で、大丈夫ですから"
    },
    {
      "role": "client",
      "time": "2020-12-31T13:52:38",
      "utterance": "お兄ちゃんやおねえちゃんばかり"

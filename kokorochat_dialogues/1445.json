    {
      "role": "client",
      "time": "2021-03-14T11:01:18",
      "utterance": "わからなくなってきて"
    },
    {
      "role": "counselor",
      "time": "2021-03-14T11:01:25",
      "utterance": "うん、うん。"
    },
    {
      "role": "client",
      "time": "2021-03-14T11:01:54",

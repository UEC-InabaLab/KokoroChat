    {
      "role": "counselor",
      "time": "2022-05-19T11:57:24",
      "utterance": "そうでしたね。"
    },
    {
      "role": "counselor",
      "time": "2022-05-19T11:57:51",
      "utterance": "どうしていいかわからないほど、苦しいんですよね。"

{
  "dialogue": [
    {
      "role": "counselor",
      "time": "2020-10-14T12:54:16",
      "utterance": "よろしくお願いいたします。"
    },
    {
      "role": "client",
      "time": "2020-10-14T12:54:51",
      "utterance": "よろしくお願いします。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T12:55:46",
      "utterance": "時間まで待ちますか"
    },
    {
      "role": "client",
      "time": "2020-10-14T12:56:06",
      "utterance": "今からでも時間からでも大丈夫ですよ。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T12:56:29",
      "utterance": "よろしくお願いいたします。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T12:57:00",
      "utterance": "今日はどのようなご相談ですか"
    },
    {
      "role": "client",
      "time": "2020-10-14T12:57:40",
      "utterance": "夫とのことで少し悩んでいます。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T12:58:51",
      "utterance": "夫さんのことですか？年齢はおいくつですか？"
    },
    {
      "role": "client",
      "time": "2020-10-14T12:59:33",
      "utterance": "私ですか？夫ですか？"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:00:02",
      "utterance": "夫さんです"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:00:28",
      "utterance": "夫の年齢、、"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:00:44",
      "utterance": "30歳ですが、、"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:01:07",
      "utterance": "はいわかりました"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:01:35",
      "utterance": "夫さんどうされましたか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:02:22",
      "utterance": "夫の息子への言い方が気になります。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:03:39",
      "utterance": "たとえばどのような時どんないいかたをするのですか？"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:04:30",
      "utterance": "片付けがてきていないときに、片付けろよ〜という普通のことなのですが、なんとかなく、キツく感じます。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:05:00",
      "utterance": "なるほど"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:06:07",
      "utterance": "男の方は声も大きいし圧迫感がありますよね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:06:26",
      "utterance": "そうですね"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:07:24",
      "utterance": "息子さんはそのことでなんとおっしゃっていますか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:08:10",
      "utterance": "得には何もいってないです。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:09:06",
      "utterance": "キツイ言い方は前からですか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:10:02",
      "utterance": "前からというな元々という感じです"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:11:27",
      "utterance": "↑いうか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:11:45",
      "utterance": "の間違いです、、"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:13:28",
      "utterance": "元々なんですね！夫さんにことばがきつくてすごくきいていてきずつとつたえてありますか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:14:04",
      "utterance": "きずつ？"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:14:29",
      "utterance": "傷つくです"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:15:23",
      "utterance": "傷ついてはいません、、"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:17:25",
      "utterance": "きつく言われるとじぶんより息子さんのきもちのほうが心配になりますよね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:18:24",
      "utterance": "まぁ、それもあります"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:20:51",
      "utterance": "男のかたは仕事できおつかうのでかぞくにはきのゆるみもありますよね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:21:15",
      "utterance": "そうですね、"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:21:52",
      "utterance": "夫さんにどうしてほしいですか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:22:36",
      "utterance": "もう少しやわらかい言い方をしてほしいですね"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:23:38",
      "utterance": "夫さんにそのままの気持ちをつたえてみてはいかがでしょうか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:24:06",
      "utterance": "もう何度も伝えています。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:26:36",
      "utterance": "そうなんですね。じゃあ夫さんが言ってるようにきついことばでかえしてみて、どうかんじるかきいてみてはいかがですか"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:27:45",
      "utterance": "すみません、、なんだか、そういうことではなくて…"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:31:24",
      "utterance": "すみません、勝手に思ってしまいました。夫さんに柔らかいいいかたをしてほしいのですね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:32:25",
      "utterance": "うーん、夫の息子への言い方が気になる自分がいるということです"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:33:44",
      "utterance": "息子さんへのいいかたをきにしているのですね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:34:26",
      "utterance": "そうです"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:36:06",
      "utterance": "息子さんに優しくふれてもらえると安心しますよね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:36:31",
      "utterance": "そうですね、、"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:37:25",
      "utterance": "夫さんには何度も伝えているのですものね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:37:38",
      "utterance": "そうです"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:40:47",
      "utterance": "息子さんにお母さんはお父さんのことばはきつく」かんじるけどどおってきいてみてはどうでしょうか？"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:41:23",
      "utterance": "聞くとそう思ってると言っています。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:42:02",
      "utterance": "そうなんですね"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:47:17",
      "utterance": "夫さんは、そのことをどこまでかんがえてくれているのか？"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:47:50",
      "utterance": "何度も伝えているので、真剣に、考えています。"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:50:18",
      "utterance": "優しい言い方にすこしずつ努力しようと考えていてくれていますね"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:51:38",
      "utterance": "うーん、それもありますが、何故私がそんなに言い方が気になるのかを考えてくれています"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:52:12",
      "utterance": "なるほど"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:53:09",
      "utterance": "ほんとうは優しい夫さんのんだすね"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:53:51",
      "utterance": "なのですね、です"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:54:25",
      "utterance": "そうですね、"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:55:34",
      "utterance": "大事な息子さんだから力が入ってしまうのですね"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T13:57:29",
      "utterance": "そう言うおとうさんに育ててもらってきっとむすこさんは、立派に成長されますよ"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:58:19",
      "utterance": "そうですね、、"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:58:25",
      "utterance": "（もうそろそろ時間ですので、今日は終わりにして頂いて大丈夫です。）"
    },
    {
      "role": "client",
      "time": "2020-10-14T13:58:31",
      "utterance": "（有難うございました。）"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T14:00:28",
      "utterance": "男同士のやり取りだとおもって温かくみまもってください。。祈ってます"
    },
    {
      "role": "counselor",
      "time": "2020-10-14T14:01:18",
      "utterance": "今日はありがとうございました。これで終了させて頂きます。"
    }
  ],

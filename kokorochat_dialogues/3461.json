    {
      "role": "counselor",
      "time": "2022-05-10T20:16:53",
      "utterance": "ええ、"
    },
    {
      "role": "client",
      "time": "2022-05-10T20:17:59",
      "utterance": "でも私の言い方も良くなかったかなとは思ってます"
    },
    {
      "role": "counselor",
      "time": "2022-05-10T20:18:11",
      "utterance": "そう思っていらっしゃるんですね"

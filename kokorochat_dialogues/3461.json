    {
      "role": "counselor",
      "time": "2022-05-10T20:12:04",
      "utterance": "ええ、"
    },
    {
      "role": "client",
      "time": "2022-05-10T20:12:08",
      "utterance": "彼はずーっと大阪の人で"
    },
    {
      "role": "client",
      "time": "2022-05-10T20:12:36",
      "utterance": "仕事はできて目標達成とする人なんですけど"
    },
    {
      "role": "counselor",
      "time": "2022-05-10T20:12:43",
      "utterance": "うんうん、"
    },
    {
      "role": "client",
      "time": "2022-05-10T20:12:44",
      "utterance": "ベースが安定思考で"
    },
    {
      "role": "client",
      "time": "2022-05-10T20:12:52",
      "utterance": "収入落としたくない、とかですね"

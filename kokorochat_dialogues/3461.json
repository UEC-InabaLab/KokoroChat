    {
      "role": "client",
      "time": "2022-05-10T20:16:37",
      "utterance": "そうなんです"
    },
    {
      "role": "client",
      "time": "2022-05-10T20:16:45",
      "utterance": "それは肯定してるんですけどね"
    },
    {
      "role": "client",
      "time": "2022-05-10T20:16:47",
      "utterance": "言葉でも"

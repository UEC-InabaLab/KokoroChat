    {
      "role": "client",
      "time": "2021-02-17T20:42:14",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-02-17T20:42:21",
      "utterance": "そのあたりは何か気がかりでもあるのでしょうか？"

    {
      "role": "client",
      "time": "2020-09-25T21:23:11",
      "utterance": "優しいとはよく言われます(笑)"
    },
    {
      "role": "counselor",
      "time": "2020-09-25T21:23:37",
      "utterance": "聞いていて私もそう感じました（笑）"

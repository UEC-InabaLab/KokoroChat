    {
      "role": "client",
      "time": "2021-02-10T10:28:54",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:29:23",
      "utterance": "リラックス出来る時間は大切だと思います。"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:29:37",
      "utterance": "僕もそう思います。"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:29:49",
      "utterance": "うんうん"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:30:06",
      "utterance": "現実に戻ると、この先人間不信を抱えたままで大丈夫かなって思うとまた不安になるんですよね。"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:30:41",
      "utterance": "うんうん そうだよね"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:30:50",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:31:03",
      "utterance": "ヒロくんの"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:31:19",
      "utterance": "やれることからで良いので"

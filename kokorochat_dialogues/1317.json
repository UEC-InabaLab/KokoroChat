    {
      "role": "client",
      "time": "2021-02-10T10:18:00",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:18:32",
      "utterance": "リラックス出来る時ってどんな時ですか？"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:18:59",
      "utterance": "うーん、本を読んだり、音楽をきいているときです。"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:19:25",
      "utterance": "そうなんだね♪"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:19:36",
      "utterance": "はい。"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:20:03",
      "utterance": "眠る前に音楽を聴くと"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:20:31",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:20:39",
      "utterance": "リラックス効果があったりするよね♫"

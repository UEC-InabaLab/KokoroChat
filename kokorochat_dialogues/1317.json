    {
      "role": "counselor",
      "time": "2021-02-10T10:34:40",
      "utterance": "うんうん"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:35:31",
      "utterance": "中学時代のクラスメイトの冷たい態度を思い出してしまって、またあんな態度をされるのではないかと怖くなるんです。"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:35:37",
      "utterance": "そう思っても怖い思いはあるよね..."
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:35:53",
      "utterance": "うんうん"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:35:55",
      "utterance": "そうなんです。なかなか動けない。"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:36:35",
      "utterance": "当時のことを思い出してしまうと"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:36:40",
      "utterance": "はい"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:37:05",
      "utterance": "怖い思いが蘇ってしまって"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:37:30",
      "utterance": "動けなくなってしまうよね..."

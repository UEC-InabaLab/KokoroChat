    {
      "role": "counselor",
      "time": "2021-02-10T10:24:45",
      "utterance": "うんうん"
    },
    {
      "role": "counselor",
      "time": "2021-02-10T10:25:01",
      "utterance": "それは良いよね！"
    },
    {
      "role": "client",
      "time": "2021-02-10T10:25:08",

    {
      "role": "client",
      "time": "2021-05-20T10:40:46",
      "utterance": "ありがとうございます。"
    },
    {
      "role": "counselor",
      "time": "2021-05-20T10:43:12",
      "utterance": "まとめさせていただきますね。"

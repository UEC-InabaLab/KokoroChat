    {
      "role": "client",
      "time": "2020-12-21T21:00:28",
      "utterance": "そうか"
    },
    {
      "role": "counselor",
      "time": "2020-12-21T21:00:56",
      "utterance": "お金がないのは、私も一緒です。"
    },
    {
      "role": "client",
      "time": "2020-12-21T21:00:59",
      "utterance": "ここの窓口もカウンセリングなんですね"
